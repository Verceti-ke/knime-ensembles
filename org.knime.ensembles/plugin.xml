            id="org.knime.ensembles.boosting.BoostingLearnerLoopStartNodeFactory">
      </node>
      <node
            category-path="/mining/ensembles/boosting"
            expert-flag="false"
            factory-class="org.knime.ensembles.boosting.BoostingLearnerLoopEndNodeFactory"
            id="org.knime.ensembles.boosting.BoostingLearnerLoopEndNodeFactory">
      </node>
      <node
            category-path="/mining/ensembles/boosting"
            expert-flag="false"
            factory-class="org.knime.ensembles.boosting.BoostingPredictorLoopStartNodeFactory"
            id="org.knime.ensembles.boosting.BoostingPredictorLoopStartNodeFactory">
      </node>
      <node
            category-path="/mining/ensembles/boosting"
            expert-flag="false"
            factory-class="org.knime.ensembles.boosting.BoostingPredictorLoopEndNodeFactory"
            id="org.knime.ensembles.boosting.BoostingPredictorLoopEndNodeFactory">
      </node>
      <node
            category-path="/mining/ensembles/bagging"
            expert-flag="false"
            factory-class="org.knime.ensembles.bagging.ModelLoopStartNodeFactory"
            id="org.knime.ensembles.bagging.ModelLoopStartNodeFactory">
