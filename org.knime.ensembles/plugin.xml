<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/missing.gif"
            level-id="ensembles"
            name="Ensemble Learning"
            path="/analytics/mining">
      </category>
      
      <category
            icon="icons/missing.gif"
            level-id="ensembles-combine"
            name="PMML"
            path="/analytics/mining/ensembles"/>
      <category
            after="ensembles-combine"
            icon="icons/metanode.png"
            level-id="meta"
            name="Meta Nodes"
            path="/analytics/mining/ensembles">
      </category>
   </extension>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            after="org.knime.base.node.preproc.pmml.xml2pmml.XML2PMMLNodeFactory"
            category-path="/manipulation/pmml"
            factory-class="org.knime.ensembles.pmmlporttocell.PMMLPortToCellNodeFactory"
            >
      </node>
      <node
            category-path="/analytics/mining/ensembles"
            factory-class="org.knime.ensembles.modeltotable.ModelToTableNodeFactory"
            >
      </node>
      <node
            category-path="/analytics/mining/ensembles"
            factory-class="org.knime.ensembles.tabletomodel.TableToModelNodeFactory"
            >
      </node>
      <node
            category-path="/analytics/mining/ensembles"
            factory-class="org.knime.ensembles.boosting.BoostingLearnerLoopStartNodeFactory"
            >
      </node>
      <node
            category-path="/analytics/mining/ensembles"
            factory-class="org.knime.ensembles.boosting.BoostingLearnerLoopEndNodeFactory"
            >
      </node>
      <node
            category-path="/analytics/mining/ensembles"
            factory-class="org.knime.ensembles.boosting.BoostingPredictorLoopStartNodeFactory"
            >
      </node>
      <node
            category-path="/analytics/mining/ensembles"
            factory-class="org.knime.ensembles.boosting.BoostingPredictorLoopEndNodeFactory"
            >
      </node>
      <node
            category-path="/analytics/mining/ensembles"
            factory-class="org.knime.ensembles.bagging.ModelLoopStartNodeFactory"
            >
      </node>
      <node
            category-path="/analytics/mining/ensembles"
            factory-class="org.knime.ensembles.bagging.ModelLoopEndNodeFactory"
            >
      </node>
      <node
            after="org.knime.base.node.preproc.pmml.xml2pmml.XML2PMMLNodeFactory"
            category-path="/manipulation/pmml"
            factory-class="org.knime.ensembles.tabletopmmlport.TableToPMMLNodeFactory"
            >
      </node>
      <node
            category-path="/analytics/mining/ensembles"
            factory-class="org.knime.ensembles.voting.VotingLoopEndNodeFactory"
            >
      </node>
      <node
            category-path="/analytics/mining/ensembles"
            deprecated="true"
            factory-class="org.knime.ensembles.delegating.DelegatingLoopStartNodeFactory">
      </node>
      <node
            category-path="/analytics/mining/ensembles"
            deprecated="true"
            factory-class="org.knime.ensembles.delegating.DelegatingLoopEndNodeFactory">
      </node>
      <node
            category-path="/analytics/mining/mining-pmml"
            deprecated="true"
            factory-class="org.knime.ensembles.pmmlpredict.PMMLPredictorNodeFactory">
      </node>
      <node
            category-path="/analytics/mining/mining-pmml"
            deprecated="true"
            factory-class="org.knime.ensembles.pmmlpredict2.PMMLPredictor2NodeFactory">
      </node>
	  <node 
	  		category-path="/analytics/mining/ensembles/ensembles-combine" 
	  		factory-class="org.knime.ensembles.pmml.combine.PMMLEnsembleNodeFactory">
      </node>
	  <node 
	  		category-path="/analytics/mining/ensembles/ensembles-combine" 
	  		factory-class="org.knime.ensembles.pmml.totable.PMMLEnsemble2TableNodeFactory">
      </node>
	  <node
			category-path="/analytics/mining/ensembles/ensembles-combine"
			deprecated="true"
			factory-class="org.knime.ensembles.pmml.predictor.PMMLEnsemblePredictorNodeFactory">
      </node>
	  <node
			category-path="/analytics/mining/ensembles/ensembles-combine"
			deprecated="true"
			factory-class="org.knime.ensembles.pmml.predictor2.PMMLEnsemblePredictor2NodeFactory">
      </node>
	  <node
	  		category-path="/analytics/mining/ensembles/ensembles-combine" 
	  		factory-class="org.knime.ensembles.pmml.loopend.PMMLEnsembleLoopEndNodeFactory">
      </node>
      <node
            category-path="/analytics/mining/ensembles"
            factory-class="org.knime.ensembles.predictionfusion.PredictionFusionNodeFactory">
      </node>
      <node
            category-path="/analytics/mining/mining-pmml"
            deprecated="false"
            factory-class="org.knime.ensembles.pmmlpredict3.PMMLPredictorNodeFactory3">
      </node>
      <node
            category-path="/analytics/mining/ensembles/ensembles-combine"
            deprecated="false"
            factory-class="org.knime.ensembles.pmml.predictor3.PMMLEnsemblePredictorNodeFactory3">
      </node>
   </extension>
   <extension
         point="org.knime.workbench.repository.metanode">
      <PersistedMetaNode
            category-path="/analytics/mining/ensembles/meta"
            description="Boosting Learner"
            icon="icons/boosting.png"
            id="org.knime.ensembles.boosting_learner"
            name="Boosting Learner"
            workflowDir="metanode_templates/Boosting Learner/">
      </PersistedMetaNode>
      <PersistedMetaNode
            category-path="/analytics/mining/ensembles/meta"
            description="Boosting Predictor"
            icon="icons/boostingpredictor.png"
            id="org.knime.ensembles.boosting_predictor"
            name="Boosting Predictor"
            workflowDir="metanode_templates/Boosting Predictor/">
      </PersistedMetaNode>
      <PersistedMetaNode
            category-path="/analytics/mining/ensembles/meta"
            description="Delegating"
            icon="icons/delegating.png"
            id="org.knime.ensembles.delegated_learning"
            name="Delegating"
            workflowDir="metanode_templates/Delegating/">
      </PersistedMetaNode>
      <PersistedMetaNode
            category-path="/analytics/mining/ensembles/meta"
            description="Bagging"
            icon="icons/bagging.png"
            id="org.knime.ensembles.bagging"
            name="Bagging"
            workflowDir="metanode_templates/Bagging/">
      </PersistedMetaNode>
   </extension>
</plugin>
